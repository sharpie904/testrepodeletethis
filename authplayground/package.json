{
  "name": "authplayground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "pnpm --filter backend dev",
    "build": "pnpm --filter backend build && pnpm --filter frontend build",
    "start": "pnpm --filter backend start",
    "db:generate": "pnpm --filter backend db:generate",
    "db:push": "pnpm --filter backend db:push",
    "db:migrate": "pnpm --filter backend db:migrate",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "api:login": "sh -c 'http -v --session=/tmp/better-auth-session POST http://localhost:3000/api/auth//sign-in/email email=\"$1\" password=\"$2\"' --",
    "api:register": "sh -c 'http POST http://localhost:3000/api/auth/sign-up/email name=\"$1\" email=\"$2\" password=\"$2\"' --",
    "api:me": "sh -c 'http -v --session=/tmp/better-auth-session http://localhost:3000/api/me'",
    "api:healthcheck": "sh -c 'http http://localhost:3000/api/healthcheck'",
    "api:logout": "sh -c 'http -v --session=/tmp/better-auth-session POST http://localhost:3000/api/auth/sign-out'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
